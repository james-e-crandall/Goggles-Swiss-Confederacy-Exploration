<?xml version="1.0" encoding="utf-8"?>
<GameEffects xmlns="GameEffects">

	<!-- SWISS_CONFEDERACY -->
	<Modifier id="TRAIT_MOD_SWISS_CONFEDERACY_LAKE_ADJACENCY" collection="COLLECTION_PLAYER_CITIES" effect="EFFECT_CITY_ACTIVATE_CONSTRUCTIBLE_ADJACENCY">
		<Argument name="ConstructibleAdjacency">SwissConfederacyLakeHappiness, SwissConfederacyLakeFood</Argument>
	</Modifier>

	<Modifier id="TRAIT_MOD_SWISS_CONFEDERACY_MOUNTAIN_WAREHOUSE" collection="COLLECTION_PLAYER_CITIES" effect="EFFECT_CITY_GRANT_WAREHOUSE_YIELD">
		<Argument name="WarehouseYieldChange">SwissConfederacyMountainWarehouseFood</Argument>
		<String context="Description">LOC_TRAIT_MOD_SWISS_CONFEDERACY_MOUNTAIN_WAREHOUSE_DESCRIPTION</String>
	</Modifier>

	<Modifier id="FURSTABTEI_MOD_WORKER_SCIENCE" collection="COLLECTION_PLAYER_CITIES" effect="EFFECT_CITY_ADJUST_WORKER_YIELD">
		<SubjectRequirements>
			<Requirement type="REQUIREMENT_CITY_HAS_UNIQUE_QUARTER">
				<Argument name="UniqueQuarterType">QUARTER_FURSTABTEI</Argument>
			</Requirement>
		</SubjectRequirements>
		<Argument name="YieldType">YIELD_SCIENCE</Argument>
		<Argument name="Amount">1</Argument>
		<Argument name="Tooltip">LOC_QUARTER_FURSTABTEI_NAME</Argument>
		<String context="Description">LOC_QUARTER_FURSTABTEI_DESCRIPTION</String>
	</Modifier>

</GameEffects>